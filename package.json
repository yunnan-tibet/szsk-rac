    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "scss": [
      "stylelint --fix"
    ],
    "*.{js,jsx,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://gitlab.com/components7/szsk-rac"
